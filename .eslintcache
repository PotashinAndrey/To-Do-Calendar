m({ ...form, [event.target.name]: event.target.value })\n  }\n\n  async function registerHandler() {\n    if (checkFields(form)) {\n      setProblems(checkFields(form))\n      return;\n    }\n\n    try {\n      await request('/api/auth/register', 'POST', { email: form.email, password: form.password, name: form.name });\n\n      history.push(\"/login\");\n    } catch (e) {\n\n      setProblems({type: 'error', text: e.message === 'email is exist yet' ? 'Аккаунт с таким email уже существует.' : 'Ошибка' });\n    }\n  }\n\n  const alertRegister = problems ? (\n    <div className=\"div-alert\">\n      <Alert\n        description={problems.text}\n        type={problems.type}\n        closable\n        onClose={onClose}\n      />\n    </div>\n  ) : (\n      null\n    )\n\n  const history = useHistory();\n\n  return (\n    <>\n      <div className=\"registration-wrapper\">\n        <div className=\"registration\">\n          <Title>Регистарция</Title>\n          <div>\n            <Typography>Введите имя: </Typography>\n            <Input\n              placeholder=\"Имя...\"\n              name=\"name\"\n              suffix={<UserOutlined />}\n              type=\"text\"\n              value={form.name}\n              onChange={e => changeFormHandler(e)}\n            />\n          </div>\n\n          <div>\n            <Typography>Введите email: </Typography>\n            <Input\n              placeholder=\"email...\"\n              name=\"email\"\n              suffix={<UserOutlined />}\n              type=\"text\"\n              value={form.email}\n              onChange={e => changeFormHandler(e)}\n            />\n          </div>\n\n          <div>\n            <Typography>Введите пароль: </Typography>\n            <Input.Password\n              placeholder=\"password...\"\n              name=\"password\"\n              iconRender={visible => (visible ? <EyeTwoTone /> : <EyeInvisibleOutlined />)}\n              type=\"password\"\n              value={form.password}\n 